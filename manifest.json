{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Leads tracker",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["style.css"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
